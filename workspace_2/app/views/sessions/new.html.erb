<h1>Member Log In</h1>
<p id="error"><%= flash[:error] %></p>
<div class="login-form">
<%=form_tag(sessions_path) do%>
    <%=label_tag 'Username'%><br/>
    <%=text_field_tag :username, params[:username]%><br/>
    <%=label_tag 'Password'%><br/>
    <%=password_field_tag :password, params[:password]%><br/>
     <div id="login-btn center">
        <button type="submit" id="login" class="transparent medium-font">Submit</button>
    </div>
<%end%>
</div>
<p id="signup">Not a Member?  <%=link_to "Sign Up Here", '/members/new'%></p>